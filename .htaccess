RewriteEngine on
RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]

RewriteRule  ^shop/page/([0-9]+)/$										        index.php?section=shop&page=$1 [L]
RewriteRule  ^shop/page/([0-9]+)$										        index.php?section=shop&page=$1 [QSA,L]
RewriteRule  ^shop/([0-9]+)/([a-zA-Z0-9_-](.+)+)/$                              index.php?section=shop&id=$1 [L]
