RewriteRule  ^shop/([0-9]+)/$										            index.php?section=shop&id=$1 [L]
RewriteRule  ^shop/([0-9]+)$													index.php?section=shop&id=$1 [QSA,L]
RewriteRule  ^shop/$												            index.php?section=shop [L]
RewriteRule  ^shop$													            index.php?section=shop [QSA,L]

